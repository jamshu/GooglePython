def array(n1,n2):
  total=n1*n2
  i=0
  j=0
  list1=[]
  list2=[]
  list3=[]
  while i<n1:
    while j<n2:
      list1.append(1)
      j=j+1
    list2.append(list1)
    i=i+1
  print list2
  return list2
def addvalue(r,c,n,list2):
  list2[r-1][c-1]=n
  print list2
